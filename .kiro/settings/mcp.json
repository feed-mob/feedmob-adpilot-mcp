{
  "mcpServers": {
    "fetch": {
      "command": "uvx",
      "args": [
        "mcp-server-fetch"
      ],
      "env": {},
      "disabled": false,
      "autoApprove": []
    },
    "exa": {
      "url": "https://mcp.exa.ai/mcp",
      "headers": {},
      "disabled": false,
      "autoApprove": [
        "get_code_context_exa"
      ]
    },
    "context7": {
      "command": "bunx",
      "args": [
        "bunx",
        "-y",
        "@upstash/context7-mcp"
      ],
      "disabled": false,
      "autoApprove": [
        "resolve-library-id",
        "get-library-docs"
      ]
    },
    "sequential-thinking": {
      "command": "bunx",
      "args": [
        "bunx",
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ],
      "disabled": false
    },
    "mcp-ui-docs": {
      "url": "https://gitmcp.io/idosal/mcp-ui",
      "disabled": false,
      "autoApprove": [
        "fetch_mcp_ui_documentation"
      ]
    },
    "agent-skills": {
      "command": "uvx",
      "args": [
        "agent-skills-mcp"
      ],
      "env": {
        "SKILL_FOLDER": "./.kiro/settings/skills"
      },
      "disabled": false,
      "autoApprove": [
        "get_skill_skill-creator"
      ]
    }
  }
}